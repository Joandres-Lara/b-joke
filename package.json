{
  "name": "b-joke",
  "version": "1.3.2",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "install:pm2": "npm install pm2@latest -g",
    "postinstall": "npm run db:migrate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate-rollback": "npx sequelize-cli db:migrate:undo:all",
    "build": "rm -rf dist && npm install -ED @babel/cli && npx babel src --out-dir dist --extensions '.js,.ts'",
    "start:dev": "npx babel-node --extensions '.ts,.js' -r dotenv/config src/index.js",
    "start:dev:services": "npm run start:dev -- --services",
    "start:dev:jobs": "npm run start:dev -- --jobs",
    "start:dev:all": "npm run start:dev -- --all",
    "start:native": "NODE_ENV=production node dist/index.js",
    "start": "npm run start:pm2",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "start:pm2-not-daemon": "pm2-runtime start ecosystem.config.js --env production",
    "pretest": "npm run db:migrate -- --env=test",
    "posttest": "npm run db:migrate-rollback -- --env=test",
    "test": "jest --coverage || true",
    "test:ci": "jest --ci --runInBand",
    "b-joke:toggle": "npx babel-node bin/index.js"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 7.19.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-async-to-generator": "7.20.7",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@types/sequelize": "4.28.14",
    "babel-plugin-module-resolver": "5.0.0",
    "chalk": "4.1.2",
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "eris": "0.17.2",
    "express": "4.18.2",
    "iconv": "3.0.1",
    "jsdom": "16.6.0",
    "node-fetch": "^2.6.9",
    "node-schedule": "2.1.1",
    "openai": "3.2.1",
    "pg": "8.10.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.30.0",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/node": "7.20.7",
    "@babel/preset-typescript": "7.21.4",
    "@babel/register": "7.21.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/xml2js": "0.4.11",
    "jest": "27.4",
    "jest-cli": "27.4",
    "jest-date-mock": "1.0.8",
    "jest-extended": "2.0.0",
    "sequelize-cli": "6.6.0",
    "supertest": "6.3.3",
    "typescript": "5.0.4"
  }
}

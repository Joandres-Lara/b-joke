{
  "name": "b-joke",
  "version": "1.0.2",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm install pm2@latest -g && npm run db:migrate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate-rollback": "npx sequelize-cli db:migrate:undo:all",
    "build": "rm -rf dist && npx babel src --out-dir dist",
    "start:dev": "npx babel-node -r dotenv/config src/index.js",
    "start": "npm run start:pm2",
    "start:pm2": "pm2-runtime start ecosystem.config.js --env production",
    "pretest": "npm run db:migrate -- --env=test",
    "posttest": "npm run db:migrate-rollback -- --env=test",
    "test": "jest --coverage || true",
    "test:ci": "jest --ci --runInBand",
    "b-joke:toggle": "npx babel-node bin/toggle-with-heroku-cli.js"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 7.19.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@types/sequelize": "^4.28.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.2",
    "date-fns": "^2.23.0",
    "dotenv": "^14.3.2",
    "eris": "^0.16.1",
    "express": "^4.17.1",
    "iconv": "^3.0.0",
    "jsdom": "^16.6.0",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/node": "^7.14.7",
    "@babel/register": "^7.14.5",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-date-mock": "^1.0.8",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4"
  }
}
